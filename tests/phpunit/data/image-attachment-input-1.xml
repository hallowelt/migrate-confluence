<xml xmlns:ac="sample_namespace" xmlns:ri="sample_second_namespace">
	<div>
		<ac:image
				ac:class="some-class"
				ac:thumbnail="true"
				ac:alt="Alternative text"
				ac:height="200"
				ac:width="500"
				ac:align="center"
		>
			<ri:attachment ri:filename="SomeImage2.png" />
		</ac:image>
	</div>
	<p>
		<ac:image ac:height="215">
			<ri:attachment ri:filename="SomeImage2.png" />
		</ac:image>
	</p>
	<p>
		<ac:image ac:height="164">
			<ri:attachment ri:filename="SomeImage2.png" />
		</ac:image>
	</p>
	<ac:link>
		<ri:page ri:content-title="SomeLinkedPage" ri:space-key="ABC" />
